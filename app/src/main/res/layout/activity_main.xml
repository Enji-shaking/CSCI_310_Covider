                android:layout_height="wrap_content"
                android:orientation="vertical">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/guiding_question1"
                    android:theme="@style/SubtitleTheme"/>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/infection"
                    android:theme="@style/PlainTextTheme"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal"
                    android:paddingEnd="20dp"
                    android:paddingStart="20dp"
                    tools:ignore="HardcodedText" >

                    <Button
                        android:id="@+id/infection_yes"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:contentDescription="infection"
                        style="?android:attr/buttonBarButtonStyle"
                        android:layout_weight="1"
                        android:text="@string/yes"
                        android:bottomRightRadius="20dp"
                        android:bottomLeftRadius="20dp"
                        android:topLeftRadius="20dp"
                        android:topRightRadius="20dp"
                        android:textColor="@color/black"
                        android:backgroundTint="@color/switch_off_track" />
                    <View
                        android:layout_width="20dp"
                        android:layout_height="0dp"
                        android:layout_weight="0.5" />
                    <Button
                        android:id="@+id/infection_no"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:contentDescription="infection"
                        style="?android:attr/buttonBarButtonStyle"
                        android:layout_weight="1"
                        android:text="@string/no"
                        android:bottomRightRadius="20dp"
                        android:bottomLeftRadius="20dp"
                        android:topLeftRadius="20dp"
                        android:topRightRadius="20dp"
                        android:textColor="@color/black"
                        android:backgroundTint="@color/switch_off_track" />
                </LinearLayout>

                <TextView
                    android:id="@+id/guiding_question2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/guiding_question2"
                    android:theme="@style/SubtitleTheme"/>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/chills_fever"
                    android:theme="@style/PlainTextTheme"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal"
                    android:paddingEnd="20dp"
                    android:paddingStart="20dp"
                    tools:ignore="HardcodedText" >

                    <Button
                        android:id="@+id/chills_fever_yes"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:contentDescription="chills_fever"
                        style="?android:attr/buttonBarButtonStyle"
                        android:layout_weight="1"
                        android:text="@string/yes"
                        android:bottomRightRadius="20dp"
                        android:bottomLeftRadius="20dp"
                        android:topLeftRadius="20dp"
                        android:topRightRadius="20dp"
                        android:textColor="@color/black"
                        android:backgroundTint="@color/switch_off_track" />
                    <View
                        android:layout_width="20dp"
                        android:layout_height="0dp"
                        android:layout_weight="0.5" />
                    <Button
