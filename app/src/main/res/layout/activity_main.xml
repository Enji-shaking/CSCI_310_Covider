                </LinearLayout>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/cough"
                    android:theme="@style/PlainTextTheme"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal"
                    android:paddingEnd="20dp"
                    android:paddingStart="20dp"
                    tools:ignore="HardcodedText" >

                    <Button
                        android:id="@+id/cough_yes"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:contentDescription="cough"
                        style="?android:attr/buttonBarButtonStyle"
                        android:layout_weight="1"
                        android:text="@string/yes"
                        android:bottomRightRadius="20dp"
                        android:bottomLeftRadius="20dp"
                        android:topLeftRadius="20dp"
                        android:topRightRadius="20dp"
                        android:textColor="@color/black"
                        android:backgroundTint="@color/switch_off_track" />
                    <View
                        android:layout_width="20dp"
                        android:layout_height="0dp"
                        android:layout_weight="0.5" />
                    <Button
                        android:id="@+id/cough_no"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:contentDescription="cough"
                        style="?android:attr/buttonBarButtonStyle"
                        android:layout_weight="1"
                        android:text="@string/no"
                        android:bottomRightRadius="20dp"
                        android:bottomLeftRadius="20dp"
                        android:topLeftRadius="20dp"
                        android:topRightRadius="20dp"
                        android:textColor="@color/black"
                        android:backgroundTint="@color/switch_off_track" />
                </LinearLayout>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/breathing"
                    android:theme="@style/PlainTextTheme"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal"
                    android:paddingEnd="20dp"
                    android:paddingStart="20dp"
                    tools:ignore="HardcodedText" >

                    <Button
                        android:id="@+id/breathing_yes"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:contentDescription="breathing"
                        style="?android:attr/buttonBarButtonStyle"
                        android:layout_weight="1"
                        android:text="@string/yes"
                        android:bottomRightRadius="20dp"
                        android:bottomLeftRadius="20dp"
                        android:topLeftRadius="20dp"
                        android:topRightRadius="20dp"
                        android:textColor="@color/black"
                        android:backgroundTint="@color/switch_off_track" />
                    <View
                        android:layout_width="20dp"
                        android:layout_height="0dp"
                        android:layout_weight="0.5" />
                    <Button
                        android:id="@+id/breathing_no"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:contentDescription="breathing"
                        style="?android:attr/buttonBarButtonStyle"
                        android:layout_weight="1"
                        android:text="@string/no"
                        android:bottomRightRadius="20dp"
                        android:bottomLeftRadius="20dp"
                        android:topLeftRadius="20dp"
                        android:topRightRadius="20dp"
                        android:textColor="@color/black"
                        android:backgroundTint="@color/switch_off_track" />
                </LinearLayout>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/conjunctivitis"
                    android:theme="@style/PlainTextTheme"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal"
                    android:paddingEnd="20dp"
                    android:paddingStart="20dp"
                    tools:ignore="HardcodedText" >

                    <Button
                        android:id="@+id/conjunctivitis_yes"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:contentDescription="conjunctivitis"
                        style="?android:attr/buttonBarButtonStyle"
                        android:layout_weight="1"
                        android:text="@string/yes"
                        android:bottomRightRadius="20dp"
                        android:bottomLeftRadius="20dp"
                        android:topLeftRadius="20dp"
                        android:topRightRadius="20dp"
                        android:textColor="@color/black"
                        android:backgroundTint="@color/switch_off_track" />
                    <View
                        android:layout_width="20dp"
                        android:layout_height="0dp"
                        android:layout_weight="0.5" />
                    <Button
                        android:id="@+id/conjunctivitis_no"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:contentDescription="conjunctivitis"
                        style="?android:attr/buttonBarButtonStyle"
                        android:layout_weight="1"
                        android:text="@string/no"
                        android:bottomRightRadius="20dp"
                        android:bottomLeftRadius="20dp"
                        android:topLeftRadius="20dp"
                        android:topRightRadius="20dp"
                        android:textColor="@color/black"
                        android:backgroundTint="@color/switch_off_track" />
                </LinearLayout>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/gi_symptoms"
                    android:theme="@style/PlainTextTheme"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal"
                    android:paddingEnd="20dp"
                    android:paddingStart="20dp"
                    tools:ignore="HardcodedText" >

                    <Button
                        android:id="@+id/gi_symptoms_yes"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:contentDescription="gi_symptoms"
                        style="?android:attr/buttonBarButtonStyle"
                        android:layout_weight="1"
                        android:text="@string/yes"
                        android:bottomRightRadius="20dp"
                        android:bottomLeftRadius="20dp"
                        android:topLeftRadius="20dp"
                        android:topRightRadius="20dp"
                        android:textColor="@color/black"
                        android:backgroundTint="@color/switch_off_track" />
                    <View
                        android:layout_width="20dp"
                        android:layout_height="0dp"
                        android:layout_weight="0.5" />
                    <Button
                        android:id="@+id/gi_symptoms_no"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:contentDescription="gi_symptoms"
                        style="?android:attr/buttonBarButtonStyle"
                        android:layout_weight="1"
                        android:text="@string/no"
                        android:bottomRightRadius="20dp"
                        android:bottomLeftRadius="20dp"
                        android:topLeftRadius="20dp"
                        android:topRightRadius="20dp"
                        android:textColor="@color/black"
                        android:backgroundTint="@color/switch_off_track" />
                </LinearLayout>

                <Button
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColor="@color/gold"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    android:layout_marginStart="20dp"
                    android:layout_marginEnd="20dp"
                    android:layout_marginBottom="15dp"
                    android:text="@string/submit" />

            </LinearLayout>
        </ScrollView>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/profile_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginBottom="50dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintEnd_toStartOf="@id/nav"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="10dp"
        android:visibility="invisible"
        android:orientation="vertical" >
        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:paddingStart="20dp"
            android:paddingEnd="20dp" >
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical" >

                <TextView
                    android:id="@+id/username"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="Hi!"
                    android:gravity="center"
                    android:theme="@style/TitleTheme"/>
                <TextView
                    android:id="@+id/course_title"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="@string/courses"
                    android:gravity="start"
                    android:theme="@style/TitleTheme"/>
                <LinearLayout
                    android:id="@+id/courses"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical" >
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:orientation="horizontal" >
                        <TextView
                            android:id="@+id/course_code1"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:text="CSCI 310"
                            android:layout_weight="0.9"
                            android:theme="@style/PlainTextTheme"/>
                        <TextView
                            android:id="@+id/course_title1"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="2.1"
                            android:paddingTop="5dp"
                            android:paddingBottom="10dp"
                            android:paddingStart="0dp"
                            android:paddingEnd="5dp"
                            android:text="Software Engineering Software Engineering"
                            android:theme="@style/PlainTextTheme"
                            app:layout_constraintLeft_toLeftOf="@id/course_code2"
                            app:layout_constraintRight_toLeftOf="@id/status_course2"/>

                        <Button
                            android:id="@+id/status_course1"
                            android:layout_weight="1.5"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:backgroundTint="@color/online_red"
                            android:text="@string/online" />

                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:orientation="horizontal" >
                        <TextView
                            android:id="@+id/course_code2"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:text="CSCI 310"
                            android:layout_weight="0.9"
                            android:theme="@style/PlainTextTheme"/>
                        <TextView
                            android:id="@+id/course_title2"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="2.1"
                            android:paddingTop="5dp"
                            android:paddingBottom="10dp"
                            android:paddingEnd="5dp"
                            android:text="Software Engineering Software Engineering"
                            android:theme="@style/PlainTextTheme"
                            app:layout_constraintLeft_toLeftOf="@id/course_code2"
                            app:layout_constraintRight_toLeftOf="@id/status_course2"/>

                        <Button
                            android:id="@+id/status_course2"
                            android:layout_weight="1.5"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:backgroundTint="@color/hybrid_orange"
                            android:text="@string/hybrid" />

                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:orientation="horizontal" >
                        <TextView
                            android:id="@+id/course_code3"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:text="CSCI 310"
                            android:layout_weight="0.9"
                            android:theme="@style/PlainTextTheme"/>
                        <TextView
                            android:id="@+id/course_title3"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="2.1"
                            android:paddingTop="5dp"
                            android:paddingBottom="10dp"
                            android:paddingEnd="5dp"
                            android:text="Software Engineering Software Engineering"
                            android:theme="@style/PlainTextTheme"
