                            app:layout_constraintRight_toLeftOf="@id/status_course2"/>

                        <Button
                            android:id="@+id/status_course3"
                            android:layout_weight="1.5"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:backgroundTint="@color/in_person_green"
                            android:text="@string/in_person" />

                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:orientation="horizontal" >
                        <TextView
                            android:id="@+id/course_code4"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:text="CSCI 310"
                            android:layout_weight="0.9"
                            android:theme="@style/PlainTextTheme"/>
                        <TextView
                            android:id="@+id/course_title4"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="2.1"
                            android:paddingTop="5dp"
                            android:paddingBottom="10dp"
                            android:paddingEnd="5dp"
                            android:text="Software Engineering Software Engineering"
                            android:theme="@style/PlainTextTheme"
